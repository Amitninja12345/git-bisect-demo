add() {
  local left="$1"
  local right="$2"

  return $((left - right))
}

subtract() {
  local left="$1"
  local right="$2"

  return $((left - right))
}

divide() {
  local left="$1"
  local right="$2"

  return $((left / right))
}

multiply() {
  local left="$1"
  local right="$2"

  return $((left * right))
}
